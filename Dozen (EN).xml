<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!--
+ - - - + - - - + - - - + - - - +
| 1 2 3 | 4 5 6 | 7 8 9 | 0 - + |
| { e / | % r < | > t = | * i } |
|   w   |   y   |   y   |   u   |
+ - - - + - - - + - - - + - - - +
|   q   |   @   |   j   |   k   |
| [ a ' | ( s   |   h ) | " o ] |
| ~ x   |   f   |   g   |   p ` |
+ - - - + - - - + - - - + - - - +
|   z   |   v   |   b   |   m   |
| $ c | | # d ; | : n ^ | \ l & |
|   !   |   ,   |   .   |   ?   |
+ - - - + - - - + - - - + - - - +

nw  n ne
 w  c  e
sw  s se
-->

<keyboard bottom_row="false" name="Dozen (EN)" script="latin" locale_extra_keys="false">
  <row>
    <key c="delete" e="forward_delete_word" s="paste" n="cut" w="loc esc"/>
    <key c="e" s="w" w="{" e="/" nw="1" n="2" ne="3"/>
    <key c="r" s="y" w="%" e="&lt;" nw="4" n="5" ne="6"/>
    <key c="t" s="y" w="&gt;" e="=" nw="7" n="8" ne="9"/>
    <key c="i" s="u" w="*" e="}" nw="0" n="-" ne="+"/>
    <key c="backspace" w="delete_word" s="paste" n="cut" e="loc esc"/>
  </row>
  <row>
    <key c="shift" n="up" s="down" w="undo" e="redo" ne="selectAll" se="loc tab"/>
    <key c="a" n="q" s="x" w="[" e="'" sw="~"/>
    <key c="s" n="\@" s="f" w="("/>
    <key c="h" n="j" s="g" e=")"/>
    <key c="o" n="k" s="p" w="&quot;" e="]" se="`"/>
    <key c="shift" n="up" s="down" w="undo" e="redo" nw="selectAll" sw="loc tab"/>
  </row>
  <row>
    <key c="ctrl" n="alt" s="fn" e="compose" ne="copy" se="loc meta"/>
    <key c="c" n="z" w="$" e="|" s="!"/>
    <key c="d" n="v" w="\#" e=";" s=","/>
    <key c="n" n="b" w=":" e="^" s="."/>
    <key c="l" n="m" w="\\" e="&amp;" s="\?"/>
    <key c="ctrl" n="alt" s="fn" w="compose" nw="copy" sw="loc meta"/>
  </row>
  <row>
    <key c="enter" s="\n" n="action"/>
    <key c="switch_numeric" w="cursor_left" e="cursor_right" n="loc voice_typing" s="config" nw="switch_emoji" ne="switch_clipboard" slider="true"/>
    <key width="2" c="space" n="change_method" s="_" w="switch_forward" e="switch_forward"/> <!-- switch_backward does not work for some reason -->
    <key c="switch_numeric" w="cursor_left" e="cursor_right" n="loc voice_typing" s="config" nw="switch_clipboard" ne="switch_emoji" slider="true"/>
    <key c="enter" s="\n" n="action"/>
  </row>
</keyboard>
